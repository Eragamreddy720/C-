#include <iostream>
using namespace std;

int main() {
    int n, number, sum = 0, i = 0;

   
    cout << "Enter the value of n: ";
    cin >> n;

   
    do {
        cout << "Enter number " << (i + 1) << ": ";
        cin >> number;
        sum += number;
        i++;
    } while(i < n);

    
    cout << "The sum of the entered numbers is: " << sum << endl;

    return 0;
}
